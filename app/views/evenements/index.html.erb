<h1>Listing evenements</h1>

<table>
  <tr>
    <th>Titre</th>
    <th>Detail</th>
    <th>Statut</th>
    <th>Date</th>
    <th>Adresse</th>
    <th>Membre</th>
    <th>Region</th>
    <th>Whodunit</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @evenements.each do |evenement| %>
  <tr>
    <td><%= evenement.titre %></td>
    <td><%= evenement.detail %></td>
    <td><%= evenement.statut %></td>
    <td><%= evenement.date %></td>
    <td><%= evenement.adresse %></td>
    <td><%= evenement.membre_id %></td>
    <td><%= evenement.region_id %></td>
    <td><%= evenement.whodunit %></td>
    <td><%= link_to 'Show', evenement %></td>
    <td><%= link_to 'Edit', edit_evenement_path(evenement) %></td>
    <td><%= link_to 'Destroy', evenement, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Evenement', new_evenement_path %>
